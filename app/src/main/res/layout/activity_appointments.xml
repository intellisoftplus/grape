<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/appointmentContainer"
    android:layout_width="match_parent" android:layout_height="match_parent">
    <fragment
        android:name="com.intellisoftplus.grape.AppointmentListFragment"
        android:id="@+id/appointmentListFrag"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout="@layout/fragment_appointments_list" />
    <fragment
        android:name="com.intellisoftplus.grape.CreateAppointmentFragment"
        android:id="@+id/createAppointmentFrag"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</RelativeLayout>